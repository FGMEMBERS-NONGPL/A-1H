<PropertyList>

  <!-- Each submodel here represents one 0.50in Browning machine gun
       firing tracers.  I don't have ballistics information for tracers,
       so I'm using data for the standard M2 bullet.
  -->


  <submodel>
    <name>left-guns-tracers</name>
    <model>Aircraft/DavePack/Stores/guns/tracer.xml</model>
    <trigger>/controls/armament/trigger</trigger>
    <speed>2840.0</speed>
    <repeat>true</repeat>
    <delay>0.15</delay>
    <count>-1</count>
    <x-offset> 3.0</x-offset>
    <y-offset>-11.0</y-offset>
    <z-offset>-1.0</z-offset>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset>0</pitch-offset>
    <life>2.0</life>
    <wind>false</wind>
